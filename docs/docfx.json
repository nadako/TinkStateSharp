{
  "metadata": [
    {
      "src": [
        {
          "files": ["TinkState/TinkState.csproj"],
          "src": "../projects"
        }
      ],
      "dest": "api",
      "shouldSkipMarkup": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "TinkState",
      "_disableContribution": true,
      "_disableNavbar": true
    }
  }
}