{
  "metadata": [
    {
      "src": [
        {
          "files": ["TinkState/TinkState.csproj"],
          "src": "../projects"
        }
      ],
      "dest": "api",
      "shouldSkipMarkup": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
		  "api/index.md"
        ]
      },
	  {
		"files": [
		  "articles/**.md",
		  "articles/**/toc.yml",
		  "*.md",
		  "toc.yml"
		]
	  }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "TinkState",
      "_disableContribution": true
    }
  }
}